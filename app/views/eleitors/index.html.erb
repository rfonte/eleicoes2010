<br /><h1>Listagem de Eleitores</h1><br /><br /><br />

<table>
  <tr>
    <th>Número do Título</th>
    <th>Código da Seção</th>
    <th>Código da Zona</th>
    <!--th>Código do funcionário</th-->
    <th>Nome</th>
    <th>Data de Nascimento</th>
    <th>CPF</th>
    <th>RG</th>
    <th>Código Biométrico</th>
  </tr>

<% for eleitor in @eleitors %>
  <tr>
    <td><%=h eleitor.tit_eleitor %></td>
    <td><%=h eleitor.cod_secao %></td>
    <td><%=h eleitor.cod_zona %></td>
    <!--td><%=h eleitor.cod_func %></td-->
    <td><%=h eleitor.nome %></td>
    <td><%=h eleitor.data_nasc %></td>
    <td><%=h eleitor.cpf %></td>
    <td><%=h eleitor.rg %></td>
    <td><%=h eleitor.cod_bio %></td>
    <td class="noborder"><%= link_to image_tag("/images/view.png", :border=>0), :action => 'show', :id => eleitor %></td>
    <td class="noborder"><%= link_to image_tag("/images/edit.png", :border=>0), :action => 'edit', :id => eleitor %></td>
    <td class="noborder"><%= link_to image_tag("/images/cancel.png", :border=>0), eleitor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to image_tag("/images/add.png", :border=>0), :action => 'new', :id => eleitor %>
<%= link_to image_tag("/images/home.png", :border=>0), :action => 'index', :controller => 'home' %>

