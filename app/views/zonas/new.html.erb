<h1>New zona</h1>

<% form_for(@zona) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cod_zona %><br />
    <%= f.text_field :cod_zona %>
  </p>
  <p>
    <%= f.label :cod_munic %><br />
    <%= f.text_field :cod_munic %>
  </p>
  <p>
    <%= f.label :cod_est %><br />
    <%= f.text_field :cod_est %>
  </p>
  <p>
    <%= f.label :nome_zona %><br />
    <%= f.text_field :nome_zona %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', zonas_path %>
