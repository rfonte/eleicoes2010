<br /><h1>Novo Município</h1><br /><br /><br />

<% form_for(@municipio) do |f| %>
  <%= f.error_messages %>
<div class="new">
  <p>Estado: <%= collection_select(:municipio, :estado_id, Estado.all, :id, :nome_est) %></p><br />
  <p>Nome do Município: <%= f.text_field :nome_munic %></p><br />
  <p class="criar"><%= f.submit "Criar" %></p><br />
<% end %>
</div>

<span class="noborder"><%= link_to image_tag("/images/voltar.png", :border=>0), :action => 'index', :controller => 'municipios' %></span>
<span class="noborder"><%= link_to image_tag("/images/home.png", :border=>0), :action => 'index', :controller => 'home' %></span>

