<h1>Edição de Municípios</h1>

<% form_for(@municipio) do |f| %>
  <%= f.error_messages %>

  <p>
    Código do Município<br />
    <%= f.text_field :cod_munic %>
  </p>
  <p>
    Código do Estado<br />
    <%= f.text_field :cod_est %>
  </p>
  <p>
    Nome do Município<br />
    <%= f.text_field :nome_munic %>
  </p>
  <p>
    <%= f.submit "Atualizar" %>
  </p>
<% end %>

<%= link_to 'Exibir', @municipio %> |
<%= link_to 'Voltar', municipios_path %>

