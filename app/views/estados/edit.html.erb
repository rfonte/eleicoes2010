<h1>Editing estado</h1>

<% form_for(@estado) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cod_est %><br />
    <%= f.text_field :cod_est %>
  </p>
  <p>
    <%= f.label :nome_est %><br />
    <%= f.text_field :nome_est %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @estado %> |
<%= link_to 'Back', estados_path %>
