<% for candidato in @candidatos %>
  <tr>
    <td><%=h candidato.cod_candidato %></td>
    <td><%=h candidato.nome_urna %></td>
    <td><%=h candidato.cpf %></td>
    <td><%=h candidato.nome_completo %></td>
    <td><%=h candidato.data_nas %></td>
    <td><%=h candidato.nacionalidade %></td>
    <td><%=h candidato.grau_instr %></td>
    <td><%=h candidato.sexo %></td>
    <td><%=h candidato.estado_civil %></td>
    <td><%=h candidato.naturalidade %></td>
    <td><%=h candidato.ocupacao %></td>
    <td class="noborder"><%= link_to image_tag("/images/view.png", :border=>0), :action => 'show', :id => candidato %></td>
    <td class="noborder"><%= link_to image_tag("/images/edit.png", :border=>0), :action => 'edit', :id => candidato %></td>
    <td class="noborder"><%= link_to image_tag("/images/cancel.png", :border=>0), candidato, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

