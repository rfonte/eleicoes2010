<br /><h1>Edição de Seção</h1><br /><br /><br />

<% form_for(@secao) do |f| %>
  <%= f.error_messages %>

<div class="editar">
  <p>Zona: <%= collection_select(:secao, :zona_id, Zona.all, :id, :nome_zona) %></p><br />
  <p>Município: <%= collection_select(:secao, :municipio_id, Municipio.all, :id, :nome_munic) %></p><br />
  <p>Estado: <%= collection_select(:secao, :estado_id, Estado.all, :id, :nome_est) %></p><br />
  <p>Urna: <%= collection_select(:secao, :urna_id, Urna.all, :id, :id) %></p><br />
  <p>Sala: <%= f.text_field :sala %></p><br />
  <p class="atualizar"><%= f.submit "Atualizar" %></p><br />
<% end %>
</div>

<span class="home"><%= link_to 'Ver', @secao %></span>
<span class="home"><%= link_to 'Voltar', secaos_path %></span>
<span class="home"><%= link_to 'HOME', home_index_path %></span>

