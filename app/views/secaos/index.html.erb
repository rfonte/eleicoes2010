<br /><h1>Listagen de Seções</h1><br /><br /><br />

<table>
  <tr>
    <th>Código da Seção</th>
    <th>Zona</th>
    <th>Município</th>
    <th>Estado</th>
    <th>Código da Urna</th>
    <th>Sala</th>
  </tr>

<% for secao in @secaos %>
  <tr>
    <td><%=h secao.id %></td>
    <td><%=h secao.zona.nome_zona %></td>
    <td><%=h secao.municipio.nome_munic %></td>
    <td><%=h secao.estado.nome_est %></td>
    <td><%=h secao.urna_id %></td>
    <td><%=h secao.sala %></td>
     <td class="noborder"><%= link_to image_tag("/images/view.png", :border=>0), :action => 'show', :id => secao %></td>
    <td class="noborder"><%= link_to image_tag("/images/edit.png", :border=>0), :action => 'edit', :id => secao %></td>
    <td class="noborder"><%= link_to image_tag("/images/cancel.png", :border=>0), secao, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to image_tag("/images/add.png", :border=>0), :action => 'new', :id => secao %>
<%= link_to image_tag("/images/home.png", :border=>0), :action => 'index', :controller => 'home' %>

